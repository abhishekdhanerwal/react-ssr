{
  "name": "new",
  "version": "1.0.0",
  "description": "ssr app",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "lint": "node_modules/.bin/eslint webpack.* src",
    "dev": "npm run lint && npm run test && npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "start": "npm run prod:server",
    "prod": "npm run lint && npm run prod:build-server && npm run prod:build-client && npm run prod:server",
    "prod:server": "node build/bundle.js",
    "prod:build-server": "webpack --config webpack.serverProd.js",
    "prod:build-client": "webpack --config webpack.clientProd.js"
  },
  "author": "",
  "keywords": [
    "heroku"
  ],
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "materialize-css": "^1.0.0-rc.2",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "victory": "^36.6.8"
  },
  "devDependencies": {
    "babel-jest": "22.4.3",
    "css-loader": "^2.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.5.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-watch": "^7.0.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "npm-run-all": "4.1.1",
    "react-addons-test-utils": "^15.6.2",
    "style-loader": "^0.13.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9",
    "webpack-node-externals": "^2.5.0"
  },
  "engines": {
    "node": "12.x"
  }
